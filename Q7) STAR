#include<graphics.h>
#include<conio.h>
int main()
{
int gd=DETECT,gm;
initgraph(&gd,&gm,"C:\\TC\\bgi");
line(150,100,100,200);
line(100,200,200,200);
line(200,200,150,100);
line(100,125,200,125);
line(100,125,150,225);
line(150,225,200,125);
setcolor(YELLOW);
outtextxy(10,20,"Name: Sumit");
outtextxy(10,40,"Rollno: 17");
getch();
closegraph();
return 0;
}

