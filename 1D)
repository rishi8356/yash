#include<graphics.h>
#include<stdlib.h>
#include<stdio.h>
#include<conio.h>
int main()
{
int gd=DETECT, gm;
initgraph(&gd,&gm, "c:\\TC\\bgi");
setcolor(RED);
circle(200,200,100);
floodfill(100,100,RED);
setcolor(YELLOW);
outtextxy(10,20,"Name: Sumit "); 
outtextxy(10,40,"Roll.No: 19 "); 
getch();
closegraph();
return 0;
}


